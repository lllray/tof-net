        "texture tex1" [ "concrete-kd-img" ] 
        "rgb tex2" [ 0.639999986 0.639999986 0.639999986 ] 
MakeNamedMaterial "concrete" 
        "string type" [ "matte" ] "rgb Kd" [ 0.62 0.62 0.62 ] 

