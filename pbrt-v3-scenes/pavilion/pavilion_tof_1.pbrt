
# LookAt 1.716 0.260 0.020 1.765 0.256 -0.067 0.002 0.100 -0.004
Scale -1 1 1
Camera "perspective" 
       "float fov" 50

Film "image" 
        "integer xresolution" 320
        "integer yresolution" 240
        "string filename" "pavilion.exr"

Sampler "random" "integer pixelsamples" [512]

WorldBegin
Scale .5 .5 .5

AttributeBegin
    CoordSysTransform "camera"
    LightSource "point" "color I" [ 100 100 100 ]
AttributeEnd

MakeNamedMaterial "Material" 
        "string type" [ "matte" ] "rgb Kd" [ 0.71 0.71 0.71 ] 

MakeNamedMaterial "None" 
        "string type" [ "matte" ] "rgb Kd" [ 0.75 0.75 0.75 ] 

